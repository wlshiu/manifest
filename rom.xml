<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- remote: name: reomte name, fetch: remote url (you can configure multi-remote, you can select remote name with project.remote) -->
  <remote name="github"
          fetch="ssh://git@github.com"
          review="git@github.com/" />

  <!-- <remote name="gitlab" -->
          <!-- fetch=".." -->
          <!-- review="git@gitlab.com" /> -->

  <!-- default: revision: branch name, remote: remote name, sync-j: sync thread number -->
  <default revision="master"
           remote="github"
           sync-j="2" />

  <!-- projectï¼špath: local relative path, name: remote path (url= remote.fetch + project.name) -->
  <project path="./driver" name="ARM-software/CMSIS-Driver" revision="master" remote="github" />
  <!-- <project path="./CMSIS-FreeRTOS" name="ARM-software/CMSIS-FreeRTOS" revision="master" remote="github" /> -->

</manifest>
